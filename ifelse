package Test;


public class ifelse{
	public static void main(String[] Args) {
		int a = 10;
		int b = 30;
		int c = a + b;
		System.out.println(c);
		
		if (a<5) {
			System.out.println("less value");
		}
		else if(b<31) {
			System.out.println("high value");
		}
		else
			System.out.println("error");
		}
	}
