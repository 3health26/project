package Test;

public class array {
	public static void main(String[] args) {
		String car[] = {"BMW", "Audi", "Toyota"};
		System.out.println(car[0]);
		
		for (int i=0; i<3; i++) { 
			System.out.println(i);
		}
	int[] number = {1,2,3,4};
	for (int i = 0; i<number.length; i++) {
		System.out.println(i);
	}
	}}
